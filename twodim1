#include <iostream>
using namespace std;
int main(){
	int n,m,i,j,a[10][10];
	cout << "Enter order of matrix";
	cin >> n >> m;
	cout << "Enter " << n << "X" << m << " matrix\n";
	for (i=1;i<=n;i++)
		for (j=1; j<=m;j++)
			cin >> a[i][j]; 
	cout << "\ngiven matrix is\n";
	for (i=1; i<=n;i++){
		for (j=1; j<=m;j++)
			cout << " " << a[i][j];
		cout << "\n";
	}	
	return 0;
}
