#include <iostream>
using namespace std;
int main(){
	int i, n, a[100];
	cout << "Enter n: ";
	cin >> n;
	cout << "Enter " << n << " numbers\n";
	for (i=0; i<n;i++){
		cin >> a[i];
	}
	cout << "ODD NUMBERS" << endl;
	for (i=0; i<n;i++){
		if (a[i]%2==1)
			cout << " " << a[i];
	}
	cout << "\nEVEN NUMBERS" << endl;
	for (i=0; i<n;i++){
		if (a[i]%2==0)
			cout << " " << a[i];
	}
	return 0;
}
